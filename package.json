    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.6.1",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.0.3",
    "concurrently": "^9.2.1",
    "hardhat": "^3.0.6",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.16.3",
