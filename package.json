{
  "name": "metasmartport",
  "version": "1.0.0",
  "type": "module",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "delegation": "node --loader ts-node/esm src/delegation/app.ts",
    "blockchain": "node --loader ts-node/esm src/config/blockchain.ts",
    "migrate": "npx prisma migrate dev --name",
    "generate": "npx prisma generate",
    "build": "tsc",
    "start": "node dist/server.js",
    "worker": "node dist/worker.js",
    "dev": "nodemon --watch src --exec ts-node src/server.ts",
    "clean": "rm -rf dist",
    "serve": "nodemon --watch src --exec \"npm run clean && npm run build && node dist/server.js\"",
    "test": "jest"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.6.1",
    "@types/supertest": "^6.0.3",
    "hardhat": "^3.0.6",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.16.3",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "private": true,
  "dependencies": {
    "@adminjs/express": "^6.1.1",
    "@adminjs/prisma": "^5.0.4",
    "@metamask/smart-accounts-kit": "^0.1.0",
    "@prisma/client": "^6.16.3",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.61.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "ethers": "^6.15.0",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "groq-sdk": "^0.33.0",
    "ioredis": "^5.8.1",
    "jsonwebtoken": "^9.0.2",
    "permissionless": "^0.2.57",
    "siwe": "^3.0.0",
    "viem": "^2.28.1"
  }
}
