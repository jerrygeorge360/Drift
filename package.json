  "type": "module",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc",
    "generate": "npx prisma generate --schema=src/schema.prisma",
    "migrate": "npx prisma migrate dev --schema=src/schema.prisma",
    "predev:all": "npm run clean && npm run generate && npm run build && npm run migrate",
    "dev:all": "concurrently \"npm run start\" \"npm run worker\" \"npm run start:poller\"",
    "start": "node dist/server.js",
    "start:poller": "node dist/poller.js",
    "worker": "node dist/worker.js",
    "test": "jest"
  },
  "prisma": {
    "schema": "src/schema.prisma"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
